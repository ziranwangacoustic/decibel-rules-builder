{
  "name": "cui-react-application",
  "version": "1.3.0",
  "description": "",
  "sideEffects": [
    "*.scss",
    "src/i18n.js"
  ],
  "main": "build/index.js",
  "publishConfig": {
    "registry": "https://artifactory.acoustic.co/artifactory/api/npm/cui-shell-npm-virtual/"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rimraf build",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest",
    "test:watch": "yarn run test --watchAll --runInBand --verbose",
    "test:update": "yarn run test --updateSnapshot",
    "test:hook": "yarn run test --silent --watchAll=false",
    "build": "yarn run clean && webpack-cli",
    "build:dev": "yarn run build --mode=development",
    "build:prod": "yarn run build --mode=production",
    "format": "prettier --write \"**/*.{scss,css,js,jsx,md,ts}\"",
    "format:diff": "prettier --list-different \"**/*.{scss,css,js,jsx,md,ts}\"",
    "start": "start:prod",
    "start:dev": "webpack-dev-server --mode=development --config-name application",
    "start:prod": "webpack-dev-server --mode=production --config-name application",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:scripts": "eslint --ext .jsx,.js .",
    "lint:scripts:hook": "eslint --ext .jsx,.js",
    "lint:styles": "stylelint --syntax scss \"./src/**/*.scss\" ",
    "semantic-release": "semantic-release",
    "parse-i18n": "i18next",
    "copyright": "node ./scripts/copyright.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test:hook"
    }
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "yarn lint:scripts:hook"
    ],
    "*.{scss,css}": [
      "yarn lint:styles"
    ],
    "*.{scss,css,js,md,jsx,json}": [
      "yarn copyright",
      "yarn format",
      "git add",
      "yarn test:hook --coverage=false --findRelatedTests"
    ]
  },
